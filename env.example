# Twilio Configuration
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_WHATSAPP_NUMBER=+1234567890
TWILIO_MESSAGING_SERVICE_SID=your_messaging_service_sid_here

# Sistema de Workspaces (para múltiples clientes)
WHATSAPP_ENABLED_WORKSPACES=1,2,3
WHATSAPP_SERVICE_TOKEN=your_service_token_here

# Workspace 1 (Cliente 1)
TWILIO_MESSAGING_SERVICE_SID__1=your_messaging_service_sid_here
TWILIO_WHATSAPP_NUMBER__1=+34603960818
WHATSAPP_WORKSPACE_NAME__1=Cliente 1

# Workspace 2 (Cliente 2)
TWILIO_MESSAGING_SERVICE_SID__2=your_messaging_service_sid_here
TWILIO_WHATSAPP_NUMBER__2=+971543381600
WHATSAPP_WORKSPACE_NAME__2=Cliente 2

# Workspace 3 (Cliente 3)
TWILIO_MESSAGING_SERVICE_SID__3=your_messaging_service_sid_here
TWILIO_WHATSAPP_NUMBER__3=+1234567890
WHATSAPP_WORKSPACE_NAME__3=Cliente 3

# n8n Webhook Configuration
# Webhook por defecto (para compatibilidad)
N8N_DEFAULT_WEBHOOK_URL=https://your-n8n-instance.com/webhook/ateneai

# Webhooks específicos por workspace (RECOMENDADO para workflows largos)
N8N_WEBHOOK_WORKSPACE_1_URL=https://your-n8n-instance.com/webhook/workspace1
N8N_WEBHOOK_WORKSPACE_1_NAME=Workspace 1
N8N_WEBHOOK_WORKSPACE_1_ENABLED=true

N8N_WEBHOOK_WORKSPACE_2_URL=https://your-n8n-instance.com/webhook/workspace2
N8N_WEBHOOK_WORKSPACE_2_NAME=Workspace 2
N8N_WEBHOOK_WORKSPACE_2_ENABLED=true

N8N_WEBHOOK_WORKSPACE_3_URL=https://your-n8n-instance.com/webhook/workspace3
N8N_WEBHOOK_WORKSPACE_3_NAME=Workspace 3
N8N_WEBHOOK_WORKSPACE_3_ENABLED=true

# Server Configuration
PORT=3000
NODE_ENV=development

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Security
JWT_SECRET=your_jwt_secret_here 